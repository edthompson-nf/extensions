              "browserTarget": "cloudz:build:production"
            },
            "development": {
              "browserTarget": "cloudz:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cloudz:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/cloudz/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/cloudz/src/favicon.ico",
              "projects/cloudz/src/assets"
            ],
            "styles": [
              "projects/cloudz/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "sharedz": {
      "projectType": "library",
      "root": "projects/sharedz",
      "sourceRoot": "projects/sharedz/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/sharedz/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/sharedz/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/sharedz/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/sharedz/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
